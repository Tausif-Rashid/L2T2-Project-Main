<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/node_modules/bootstrap/dist/css/bootstrap.css" />
    <title>Document</title>
    <style>
    </style>
</head>

<body>
    <div class="text-center container div-scroll"
        style="margin-top:30px ; height: auto; width : auto; background-color:rgb(148, 189, 218); margin-bottom: 30px;"
        id="div1">
        <button class="btn btn-dark dropdown-toggle" type="button" id="studentIDForResultButton" data-bs-toggle="dropdown"
            aria-expanded="false">
            Select Student ID
        </button>
        <ul class="dropdown-menu dropdown-menu-dark" aria-labelledby="studentIDForResultButton">
            <% students.rows.forEach(student=> { %>
                <li><a class="dropdown-item studentIDForResultAnchor" href="#">
                        <%= student.STUDENT_ID %>
                    </a></li>
                <% }); %>
        </ul>
        <div class="text-center container"
            style="margin-top: 30px; height: auto; width : auto; background-color:rgb(148, 189, 218) ;" id="div3">
        </div>
    </div>
    <script src="/node_modules/bootstrap/dist/js/bootstrap.bundle.js"></script>
</body>


</html>